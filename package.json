{
  "name": "reacto-form",
  "version": "0.0.2",
  "description": "A reference implementation of the Composable Form Specification for React (see forms.dairystatedesigns.com)",
  "author": "Dairy State Designs (http://dairystatedesigns.com/)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/DairyStateDesigns/reacto-form.git"
  },
  "bugs": {
    "url": "https://github.com/DairyStateDesigns/reacto-form/issues"
  },
  "homepage": "https://github.com/DairyStateDesigns/reacto-form",
  "files": [
    "CHANGELOG.md",
    "dist",
    "LICENSE",
    "README.md"
  ],
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "react"
    ]
  },
  "eslintConfig": {
    "extends": "airbnb",
    "parser": "babel-eslint",
    "env": {
      "browser": true,
      "jest": true
    },
    "rules": {
      "arrow-body-style": 0,
      "consistent-return": 0,
      "max-len": 0,
      "no-param-reassign": 0,
      "no-underscore-dangle": 0,
      "no-use-before-define": [
        2,
        "nofunc"
      ],
      "no-unused-expressions": 0,
      "no-console": 0,
      "space-before-function-paren": 0,
      "react/prefer-stateless-function": 0,
      "react/no-multi-comp": 0,
      "jsx-a11y/href-no-hash": "off",
      "jsx-a11y/anchor-is-valid": [
        "warn",
        {
          "aspects": [
            "invalidHref"
          ]
        }
      ]
    }
  },
  "jest": {
    "setupFiles": [
      "raf/polyfill",
      "<rootDir>/jestSetup.js"
    ]
  },
  "main": "./dist/index.js",
  "scripts": {
    "build": "rm -rf dist/** && babel lib --out-dir dist --ignore *.test.js,__snapshots__",
    "lint": "eslint .",
    "prepare": "npm run lint && npm test && npm run build",
    "test": "jest"
  },
  "peerDependencies": {
    "react": ">=15"
  },
  "dependencies": {
    "clone": "2.1.1",
    "lodash.get": "4.4.2",
    "lodash.isempty": "4.4.0",
    "lodash.isequal": "4.5.0",
    "lodash.set": "4.3.2",
    "lodash.topath": "^4.5.2",
    "lodash.union": "^4.6.0",
    "lodash.uniqueid": "4.0.1",
    "lodash.unset": "4.5.2",
    "lodash.without": "^4.4.0",
    "prop-types": "^15.6.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^21.2.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "composable-form-tests": "^0.1.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.7.0",
    "jest": "^22.4.3",
    "moment-timezone": "^0.5.14",
    "raf": "^3.4.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-test-renderer": "^16.2.0"
  }
}
